{
  "name": "robot-in-a-can-framework",
  "version": "2022.11.16-00.43.52",
  "description": "Robot in a Can website",
  "license": "MIT",
  "homepage": "https://codepen.io/openco/pen/bGKgadB",
  "scripts": {
    "start": "cd src && bunx @11ty/eleventy --serve --output=../dist",
    "admin": "npx decap-server & cd src && bunx @11ty/eleventy --serve --output=../dist",
    "build": "cd src && bunx @11ty/eleventy --output=../dist",
    "buildfresh": "rm -rf dist && mkdir dist && cd src && bunx @11ty/eleventy --output=../dist",
    "srvsrc": "cd src && python -m http.server 8000 && cd .."
  },
  "config": {
    "srcDir": "src/",
    "distDir": "dist/"
  },
  "browserslist": [
    "last 5 version"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "luxon": "^3.6.1",
    "markdown-it-anchor": "^8.6.7"
  },
  "dependencies": {
    "markdown-it-attrs": "^4.1.6",
    "feather-icons": "^4.29.0"
  }
}
